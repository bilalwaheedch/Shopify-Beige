.predictive__screen:after {
    content: "";
    background: red;
    height: 100vh;
    width: 50%;
    display: block;
    margin-left: auto;
    background: white;
    border: 1px solid black;
}

.newslatter-section {
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 2em 0;
  border-right: 0;
  border-left: 0;
  margin: 1em 0;
}

.newslatter-section a {
  text-decoration: underline;
  font-size: 14px;
}

.newslatter-section p {
  margin: 10 PX 0;
}

.main-video-new video {
  width: 99%;
}


